{
    "name": "jordandalton/anthropic-sdk-php",
    "description": "This is my package anthropic-sdk-php",
    "keywords": [
        "JordanDalton",
        "anthropic-sdk-php"
    ],
    "homepage": "https://github.com/jordandalton/anthropic-sdk-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Jordan Dalton",
            "email": "jordan.dalton@ymail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "saloonphp/saloon": "^2.0"
    },
    "require-dev": {
        "pestphp/pest": "^2.15",
        "laravel/pint": "^1.0",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "JordanDalton\\AnthropicSdkPhp\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JordanDalton\\AnthropicSdkPhp\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
            "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    }
}
